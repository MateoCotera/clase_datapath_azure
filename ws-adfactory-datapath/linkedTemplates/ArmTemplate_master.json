{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ws-adfactory-datapath"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://mateo.vault.azure.net/"
		},
		"ls_adls_datapath_azure_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adatalakestoragedatapath.dfs.core.windows.net/"
		},
		"ls_fl_azure_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\data_adf"
		},
		"ls_fl_azure_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "mateo"
		},
		"ls_kay_vault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyproyectofinaldatapath.vault.azure.net/"
		},
		"trigger_master_properties_pipeline_parameters_Nombrearchivo": {
			"type": "string",
			"defaultValue": "2021.csv"
		},
		"trigger_master_properties_pipeline_parameters_FechaMinima": {
			"type": "string",
			"defaultValue": "2021-11-01"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ws-adfactory-datapath_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"ls_kay_vault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_kay_vault_properties_typeProperties_baseUrl')]"
					},
					"ls_adls_datapath_azure_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_datapath_azure_properties_typeProperties_url')]"
					},
					"ls_fl_azure_properties_typeProperties_host": {
						"value": "[parameters('ls_fl_azure_properties_typeProperties_host')]"
					},
					"ls_fl_azure_properties_typeProperties_userId": {
						"value": "[parameters('ls_fl_azure_properties_typeProperties_userId')]"
					},
					"trigger_master_properties_pipeline_parameters_Nombrearchivo": {
						"value": "[parameters('trigger_master_properties_pipeline_parameters_Nombrearchivo')]"
					},
					"trigger_master_properties_pipeline_parameters_FechaMinima": {
						"value": "[parameters('trigger_master_properties_pipeline_parameters_FechaMinima')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}